<script type="text/javascript">
    function getTreeData(){
        var tree_data = "";
        $.ajax({
            url:"<%=request.getContextPath()%>/Tree/queryTree",
            type:"post",
            dataType:"json",
            async:false,
            success:function(data){
                tree_data = data;
            },
            error:function(){
                alert("查询出错");
            }
        });
        return tree_data;
    }

    $(function(){
        $.ajax({
            url:"<%=basePath%>/toMain",
            type : "post",
            success : function(data) {
                //                     				添加选项卡面板
                $.addtabs.add({
                    id : 0,
                    title : "首页",
                    content : data,
                })
            }
        })
        $('#left-tree').treeview({
                data:getTreeData(),
                /* onNodeCollapsed:function(event,node){
                       alert("折叠了");
                   },
                  onNodeExpanded :function(event,node){
                      alert("打开了");
                  }, */
                onNodeSelected:function(event,node){

                    if(node.href != null && node.href != ""){

                        $.ajax({
                            url:"<%=basePath%>/" + node.href,
                            type : "post",
                            success : function(data) {
                                //                     				添加选项卡面板
                                $.addtabs.add({
                                    id : node.id,
                                    title : node.text,
                                    content : data,
                                })
                            }
                        })
                    }
                }
            });

    })
</script>
